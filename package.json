    "bootstrap-icons": "^1.10.3",
    "bootstrap-sass": "^3.4.3",
    "bootstrap-slider": "^11.0.2",
    "bootstrap-toggle": "2.2.2",
    "chart.js": "^2.9.3",
    "crypto-js": "^4.1.1",
