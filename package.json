    "mini-css-extract-plugin": "^0.12.0",
    "mocha": "^10.2.0",
    "mochapack": "^2.1.4",
    "mock-local-storage": "^1.1.23",
    "node-fetch": "^3.3.0",
    "npm-run-all": "^4.1.5",
    "null-loader": "^4.0.1",
    "openssl-nodejs": "^1.0.5",
    "raf": "^3.4.1",
    "regenerator-runtime": "^0.14.0",
    "replace-in-file": "^6.3.5",
    "resize-observer-polyfill": "^1.5.1",
    "sass": "1.77.8",
    "sass-loader": "^10",
    "selenium-webdriver": "^4.6.1",
    "shelljs": "^0.8.5",
    "sinon": "^15.0.2",
    "sinon-chai": "^3.7.0",
    "svg-url-loader": "^7.1.1",
    "text-loader": "^0.0.1",
    "vue-loader": "^15.9.8",
    "webpack": "^4.47.0",
    "webpack-bundle-analyzer": "^4.9.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.3",
    "webpack-merge": "^4.2.2",
    "worker-loader": "^3.0.8",
    "xmlserializer": "^0.6.1",
    "zip-a-folder": "^1.1.5"
  },
  "overrides": {
    "@cesium/engine": "npm:dry-uninstall",
    "@cesium/widgets": "npm:dry-uninstall"
  },
  "engines": {
    "node": "^16.13.2 || ^18.16.0 || ^20.0.0",
    "npm": "^8.1.2 || ^9.5.1"
  },
  "browser": {
    "./src/modules/tools/newDatasets/components/NewDatasets.vue": "./src/modules/tools/newDatasets/components/NewDatasets.vue",
    "./src/modules/tools/newDatasets/store/indexNewDatasets": "./src/modules/tools/newDatasets/store/indexNewDatasets"
  }
}

